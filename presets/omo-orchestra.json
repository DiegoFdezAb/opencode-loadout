{
  "$schema": "../schema/preset.schema.json",
  "name": "omo-orchestra",
  "description": "EXPERIMENTAL - Combined Oh-My-OpenCode + Open Orchestra for maximum orchestration",
  "plugins": ["oh-my-opencode", "opencode-orchestrator"],
  "requires": {
    "oh-my-opencode": "latest",
    "opencode-orchestrator": "latest"
  },
  "config": {
    "opencode": {
      "plugin": ["oh-my-opencode", "opencode-orchestrator"]
    },
    "omo": {
      "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
      "sisyphus_agent": {
        "disabled": false,
        "builder_enabled": true,
        "planner_enabled": true
      },
      "agents": {
        "Sisyphus": {
          "model": "anthropic/opus-4.5",
          "temperature": 0.7
        },
        "explore": {
          "model": "opencode/grok-code",
          "temperature": 0.2
        }
      }
    },
    "orchestra": {
      "autoSpawn": false,
      "workers": ["vision", "docs", "coder"]
    }
  }
}
