{
  "$schema": "../schema/preset.schema.json",
  "name": "omo",
  "description": "Oh-My-OpenCode with Sisyphus orchestrator and default agents",
  "plugins": ["oh-my-opencode"],
  "requires": {
    "oh-my-opencode": "latest"
  },
  "config": {
    "opencode": {
      "plugin": ["oh-my-opencode"]
    },
    "omo": {
      "sisyphus_agent": {
        "disabled": false,
        "builder_enabled": false,
        "planner_enabled": true,
        "replace_build": true,
        "replace_plan": true
      }
    }
  }
}
